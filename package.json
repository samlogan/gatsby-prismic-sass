{
  "name": "gatsby-prismic-starter",
  "description": "A lean Gatsby Starter which uses the Headless CMS Prismic",
  "license": "MIT",
  "version": "1.0.0",
  "author": "Sam Logan",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop -o",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "format": "prettier \"**/*.md \" --write"
  },
  "dependencies": {
    "gatsby": "^2.13.50",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-brotli": "^1.0.0",
    "gatsby-plugin-canonical-urls": "^2.0.10",
    "gatsby-plugin-create-client-paths": "^2.1.3",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-google-tagmanager": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-netlify": "^2.0.11",
    "gatsby-plugin-netlify-cache": "^1.0.0",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-sitemap": "^2.0.6",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-source-prismic": "3.0.0-beta.4",
    "gatsby-transformer-sharp": "^2.1.13",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-alice-carousel": "^1.15.3",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "typeface-lato": "^0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.2",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby",
    "starter",
    "prismic",
    "minimal",
    "gatsby-starter"
  ]
